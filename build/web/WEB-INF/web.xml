<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <servlet>
        <servlet-name>first</servlet-name>
        <servlet-class>servletPackage.servletClass</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>first</servlet-name>
        <url-pattern>/servletClass</url-pattern>
    </servlet-mapping>
    
    <!--    success servlet-->
    <servlet>
        <servlet-name>success</servlet-name>
        <servlet-class>servletPackage.successServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>success</servlet-name>
        <url-pattern>/successServlet</url-pattern>
    </servlet-mapping>
    
    <!--    s1 servlet-->
    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>ArritibuteServlet.s1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/s1</url-pattern>
    </servlet-mapping>
 
    <!--    s2 servlet-->
    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>ArritibuteServlet.s2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/s2</url-pattern>
    </servlet-mapping>
    
   <!--register servlet-->
   <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>RegisterSystem.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
      
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
